<h1>Cargue su CSV de Presupuesto</h1>
<div class="block-form-content">
  <%= flash[:notice] %>
  <div>
    <%= form_tag import_presupuestos_path, multipart: true do %>
      <div class="field">
        <%= file_field_tag :file %>
      </div>
      <div class="actions">
        <%= submit_tag "Cargar Archivo CSV" %>
      </div>
    <% end %>
  </div>
</div>