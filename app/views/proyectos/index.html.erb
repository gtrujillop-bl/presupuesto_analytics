<p id="notice"><%= notice %></p>

<h1>Proyectos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Fecha inicio</th>
      <th>Fecha fin</th>
      <th>Facultad</th>
      <th>Grupo</th>
      <th>Semillero</th>
      <th>Investigador</th>
      <th>Numero proyecto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proyectos.each do |proyecto| %>
      <tr>
        <td><%= proyecto.nombre %></td>
        <td><%= proyecto.fecha_inicio %></td>
        <td><%= proyecto.fecha_fin %></td>
        <td><%= proyecto.facultad_id %></td>
        <td><%= proyecto.grupo_id %></td>
        <td><%= proyecto.semillero_id %></td>
        <td><%= proyecto.investigador_id %></td>
        <td><%= proyecto.numero_proyecto %></td>
        <td><%= link_to 'Show', proyecto %></td>
        <td><%= link_to 'Edit', edit_proyecto_path(proyecto) %></td>
        <td><%= link_to 'Destroy', proyecto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proyecto', new_proyecto_path %>
